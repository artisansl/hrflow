{
	"name": "web",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"engineStrict": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"test": "echo \"no test yet\"",
		"preinstall": "npx only-allow npm",
