		"@tailwindcss/vite": "^4.1.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.25.6",
		"svelte-check": "^4.1.5",
